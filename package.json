  "license": "ISC",
  "dependencies": {
    "hugh": "^0.4.0",
    "johnny-five": "^0.9.18",
    "moment": "^2.11.1"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-eslint": "^5.0.0-beta6",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.3.13",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.1.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^4.0.0",
    "eslint-plugin-react": "^3.11.3",
    "jscs": "^2.7.0",
